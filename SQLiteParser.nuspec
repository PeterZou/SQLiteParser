<?xml version="1.0"?>
<package >
  <metadata>
    <id>Outercurve.SQLiteParser</id>
    <version>$version$</version>
    <title>SQLite CREATE Statement Parser for .Net</title>
    <authors>$author$</authors>
    <owners>$author$</owners>
    <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>$projecturl$</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>SQLite CREATE Statement Parser for .Net is a library for parsing and manipulating SQLite CREATE TABLE and CREATE INDEX statements and simulate ALTER TABLE support for SQLite.</description>
    <copyright>Copyright 2014</copyright>
    <dependencies>
      <dependency id="Antlr4" version="4.1.0-alpha003"/>
      <dependency id="Antlr4.Runtime" version="4.1.0-alpha003"/>
    </dependencies>
  </metadata>
  <files>
    <file src="SQLiteParseTreeCreator\bin\$configuration$\*.dll" target="lib" exclude="**/Antlr4*.dll" />
    <file src="SQLiteParseTreeCreator\bin\$configuration$\*.pdb" target="lib"/>
    <file src="SQLiteParseTreeCreator\bin\$configuration$\*.xml" target="lib" exclude="**/Antlr4*.xml"/>
  </files>
</package>